{
  "name": "photo2profit-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for Photo2Profit AI - Remove.bg processing and image handling",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/node": "^20.8.0"
  },
  "private": true
}